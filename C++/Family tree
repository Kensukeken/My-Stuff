
#include <iostream>
#include <string>
using namespace std;

int main() {
  const int numOfFamilyMembers = 7;
  
  int numbers[] = {0, 2, 3, 4, 5, 6, 7};
  
  string names[] = {"Khaled", "Meral", "Diaa", "Hala", "Haya", "Maya", "Mayar"};
  
  string relationships[] = {"Father", "Mother", "Brother", "Sister", "Brother", "Sister", "Sister"};
  
  for (int i = 0; i < numOfFamilyMembers; i++) {
    cout << "Family member #" << numbers[i] << ": " << names[i] << " (" << relationships[i] << ")" << endl;
  }

  return 0;
}

